        "/users": {
            "post": {
                "tags": ["User"],
                "summary": "Create a user",
                "description": "Creates a new user",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "password": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "cpf": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "email": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "state": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "city": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "isPolitician": {
                                        "type": "string",
                                        "required": false
                                    },
                                    "occupation": {
                                        "type": "string",
                                        "required": false
                                    }
                                },
                                "example": {
                                    "name": "Test",	
                                    "password": "1234",
                                    "cpf": "000000000000",
                                    "email": "test@test.com",
                                    "state": "AM",
                                    "city": "Taubaté",
                                    "isPolitician": "false",
                                    "occupation": "teste"
                                }
                            }
                        }
                    },
                    "required": "true",
                    "description": "Required fields"
                },
                "responses": {
                    "201": {
                        "description": "User created"                   
                    },
                    "400": {
                        "description": "User already exists!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",                                    
                                    "example": {
                                        "error": "User already exists!"
                                    }                                    
                                }
                            }
                        }
                    }
                }
            },     
