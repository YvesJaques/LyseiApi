        "/users": {
            "post": {
                "tags": ["User"],
                "summary": "Create a user",
                "description": "Creates a new user",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "password": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "cpf": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "email": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "state": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "city": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "isPolitician": {
                                        "type": "string",
                                        "required": false
                                    },
                                    "Occupation": {
                                        "type": "string",
                                        "required": false
                                    }
                                },
                                "example": {
                                    "name": "Test",	
                                    "password": "1234",
                                    "cpf": "000000000000",
                                    "email": "test@test.com",
                                    "state": "AM",
                                    "city": "Taubaté"
                                }
                            }
                        }
                    },
                    "required": "true",
                    "description": "Required fields"
                },
                "responses": {
                    "201": {
                        "description": "User created"                   
                    },
                    "400": {
                        "description": "User already exists!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",                                    
                                    "example": {
                                        "error": "User already exists!"
                                    }                                    
                                }
                            }
                        }
                    }
                }
            },     
            "get": {
                "tags": ["User"],
                "summary": "Show user profile",
                "description": "User must be authenticated. Can only be accessed by the actual user",
                "security": [
                    { "bearerAuth": []}
                ],               
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "name": {
                                            "type": "string",
                                            "required": true
                                        },
                                        "email": {
                                            "type": "string",
                                            "required": true
                                        }
                                    },
                                    "example": {
                                        "name": "Teste",
                                        "cpf": "00000000000",
                                        "email": "teste@teste.com",
                                        "avatar": "9efsfsdgcd0377b7ceb9bb726f12e0b-1564313132932.jpeg",
                                        "avatar_url": "https://api-lysei.s3.sa-east-1.amazonaws.com/avatar/9e456416717ccd039b7ceb9bb726f12e0b-15146543132932.jpeg",
                                        "state": "AM",
                                        "city": "Taubaté",
                                        "isPolitician": false,
                                        "occupation": null,
                                        "created_at": "2021-06-20T04:53:51.095Z"
                                    }                                
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Invalid token!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",                                    
                                    "example": {
                                        "error": "Invalid token!"
                                    }                                    
                                }
                            }
                        }
                    }
                }
            },
            "patch": {
                "tags": ["User"],
                "summary": "Update user profile",
                "description": "User must be authenticated",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "required": true
                                    },                                  
                                    "cpf": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "email": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "state": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "city": {
                                        "type": "string",
                                        "required": true
                                    },
                                    "isPolitician": {
                                        "type": "string",
                                        "required": false,
                                        "default": false
                                    },
                                    "Occupation": {
                                        "type": "string",
                                        "required": false,
                                        "default": null
                                    }
                                },
                                "example": {
                                    "name": "Test",
                                    "cpf": "000000000000",
                                    "email": "test@test.com",
                                    "state": "AM",
                                    "city": "Taubaté",
                                    "isPolitician": false,
                                    "occupation": null
                                }
                            }
                        }
                    }                    
                },
                "responses": {
                    "201": {
                        "description": "Created"                   
                    },
                    "400": {
                        "description": "Email or CPF already in use!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",                                    
                                    "example": {
                                        "error": "Email or CPF already in use!"
                                    }                                    
                                }
                            }
                        }
                    }
                }
            }
        },
        "/users/resetPassword": {
            "patch": {
                "tags": ["User"],
                "summary": "Update user password",
                "description": "User must be authenticated",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "password": {
                                        "type": "string",
                                        "required": true
                                    }
                                },
                                "example": {
                                    "password": "Test"                                   
                                }
                            }
                        }
                    }                    
                },
                "responses": {
                    "200": {
                        "description": "Updated"                   
                    },
                    "401": {
                        "description": "Invalid token!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",                                    
                                    "example": {
                                        "error": "Invalid token!"
                                    }                                    
                                }
                            }
                        }
                    }
                }
            }
        },
        "/users/avatar": {
            "patch": {
                "tags": ["User"],
                "summary": "Update user avatar",
                "description": "User must be authenticated",
                "security": [
                    { "bearerAuth": []}
                ],
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "images": {
                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "format": "binary"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "204": {
                        "description": "No Content"                   
                    },
                    "401": {
                        "description": "Invalid token!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",                                    
                                    "example": {
                                        "error": "Invalid token!"
                                    }                                    
                                }
                            }
                        }
                    }
                }
            }
        }   
    },
    "definitions": {

    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    }
}